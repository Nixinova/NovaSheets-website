<table>
    <thead>
        <tr>
            <th>Version</th>
            <th>Released</th>
            <th>Changelog</th>
            <th colspan="2" class="desktoponly">Download</th>
            <th>Docs</th>
        </tr>
    </thead>
    {% assign repo = "https://github.com/NovaSheets/NovaSheets" -%}
    {% for item in versions -%}
     {% if forloop.index != versions.length -%}
        {% assign ver  = item | slice: 1,10  | strip -%}
        {% assign date = item | slice: 12,10 | strip -%}
        {% assign docs = item | slice: 23,4  | strip -%}
        {% assign hash = item | slice: 28,40 | strip -%}
        {% assign patch = ver | slice: 4,2 %}
        {% assign pre = ver | slice: 5,1 %}
        <tr
         {%- if patch == '0' %} style="background: #0004;" {% endif -%}
         {%- if pre != '-' %} style="background: #2224;"
         {% else %} style="background: #4444;"
         {% endif -%}
        >
            <td>{{ver}}</td>
            <td>{{date}}</td>
            <td><a href="/changelog/#{{ver | replace: '.', ''}}">View</a></td>
            <td class="desktoponly"><a href="/src/{{ver}}">Full</a></td>
            <td class="desktoponly"><a href="/src/{{ver}}/min">Min</a></td>
            <td>
             {% if docs == "rdme" -%}
                <a href="{{repo}}-website/blob/{{ver}}/readme.md">View</a>
             {% elsif docs == "wiki" -%}
                <a href="{{repo}}/wiki/Syntax/{{hash}}">Wiki</a>
             {% elsif docs == "gtag" -%}
                <a href="{{repo}}-website/tree/{{ver}}/pages/docs">Browse</a>
             {% elsif docs == "docs" -%}
                <a href="/docs/">Site</a>
             {% endif -%}
            </td>
        </tr>
      {% endif -%}
    {% endfor -%}
</table>
